@model Domain.PositionName

@{
    ViewData["Title"] = "Details";
}

<h2>@Resources.Common.Details</h2>

<div>
    <h4>PositionName</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.PositionNameName)
        </dt>
        <dd>
            @Html.Raw(Model.PositionNameName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.PositionNameId">@Resources.Common.Edit</a> |
    <a asp-action="Index">@Resources.Common.BackToList</a>
</div>
